{
  "name": "@streamdal/snitch-node-client",
  "workspaces": [
    "examples"
  ],
  "version": "0.1.5",
  "description": "Streamdal's Snitch Node Client",
  "keywords": [
    "snitch",
    "streamdal",
    "typescript",
    "node",
    "data quality"
  ],
  "type": "module",
  "exports": {
    "./*": {
      "types": "./types/*.d.ts",
      "require": "./cjs/*.js",
      "import": "./esm/*.js",
      "default": "./esm/*.js"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:main && ./generatePackages.sh",
    "build:main": "npx tsc -p tsconfig.types.json && npx tsc -p tsconfig.esm.json && npx tsc -p tsconfig.cjs.json",
    "copy:package": "mkdir -p ./build && cp package.json ./build/package.json",
    "clean": "rimraf build",
    "sandbox": "tsc-watch --onSuccess \"node --experimental-wasi-unstable-preview1 ./build/sandbox/index.js\"",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "type:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamdal/snitch-node-client.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streamdal/snitch-node-client/issues"
  },
  "homepage": "https://github.com/streamdal/snitch-node-client#readme",
  "dependencies": {
    "@grpc/grpc-js": "1.8.17",
    "@grpc/proto-loader": "0.7.7",
    "@protobuf-ts/grpc-transport": "2.9.0",
    "@protobuf-ts/runtime-rpc": "2.9.0",
    "@streamdal/snitch-protos": "0.0.101",
    "@types/rwlock": "^5.0.3",
    "@types/sinon": "^10.0.16",
    "dotenv": "^16.3.1",
    "rwlock": "^5.0.0",
    "sinon": "^15.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "20.7.1",
    "@types/prompts": "^2.4.4",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "source-map-support": "^0.5.21",
    "tsc-watch": "6.0.4",
    "tsx": "3.12.7",
    "typescript": "5.1.3",
    "vitest": "^0.32.2"
  }
}
